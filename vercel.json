{
    "version": 2,
    "name": "todoApp-backend",
    "builds": [{ "src": "server.js", "use": "@vercel/node" }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.js",
            "headers": {
                "Access-Control-Allow-Origin": "https://todo-app-frontend-beryl.vercel.app",
                "Access-Control-Allow-Methods": "GET, POST"
            }
        }
    ]
}